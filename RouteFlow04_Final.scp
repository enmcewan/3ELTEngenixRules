    SysRouteInsertStepExt('OL_DEV', 'OL_DEV_IS');
    SysRouteInsertStepExt('Mdl_Insp_OLDev_IS', 'Mdl_Insp_OLDev_IS');
    SysRouteInsertStepExt('Immersion_Silver', 'Immersion_Silver01');
    SysRouteInsertStepExt('IS_Strip', 'IS_Strip01'); 
  end;
end;

procedure Lead_Free_HAL_Sel_Gold(process: string); // 1355 Could maybe consolidate this with Immersion Silver + Gold at some point
begin
  if process = 'Taping'  then
  begin                                                                                                        
    SysRouteInsertStepExt('HAL', 'HAL01');
  end
  else if process = 'Photo' then 
  begin
    Baking;
    
    if BakingDone then
      SysRouteInsertStepExt('OffLine_PreTreat', 'OffLine_PreTreat01');
                                                                       
    SysRouteInsertStepExt('OL_Photo_Selective_Finishing', 'OL_Photo_Selective_Finishing01');
    SysRouteInsertStepExt('OL_DEV', 'OL_DEV_IS');
    SysRouteInsertStepExt('Mdl_Insp_OLDev_IS', 'Mdl_Insp_OLDev_IS');
    SysRouteInsertStepExt('HAL', 'HAL01');
    SysRouteInsertStepExt('LFH_Strip', 'LFH_Strip01');  
  end;
  
  SysRouteInsertStepExt('Hole_Check', 'Hole_Check01');
  HoleCheckDone := true;
end;

procedure VCut;
begin
  if (
      (Fnc.SysGetAttrValue('LayupThickness') <= 0.65) or
      (Fnc.SysGetAttrValue('DeWarp') = true) or
      (
       (
        ((Fnc.SysGetAttrValue('CoreMat') = 'MCL-BE-67G(H)') or (Fnc.SysGetAttrValue('PPGMat') = 'MCL-BE-67G(H)')) or
        ((Fnc.SysGetAttrValue('CoreMat') = 'NPG-TL') or (Fnc.SysGetAttrValue('PPGMat') = 'NPG-TL'))
       )
       and
       (Fnc.SysGetAttrValue('LayupThickness') <= 1.0)
      )
     )                        
  then
    SysRouteInsertStepExt('DeWarp', 'DeWarp01');

  SysRouteInsertStepExt('VCut', 'VCut01');

  VCutDone := true;

end;

procedure Immersion_TinPt2; // Immersion Tin Changes
begin
  if BakingDone = false then
    Baking;

  if ((Fnc.SysGetAttrValue('BackDrill') = true) and (BackDrillDone = false)) then  //Make sure this does not get done multiple times
    Back_Drill;

  if (     
      (Fnc.SysGetAttrValue('Gold_Tab_Rows') <= 2) and
      (VCUT_TYPE = 'Panel') and
      (VCutDone = false)
     )
  then
    VCut;

  if Fnc.SysGetAttrValue('LayupThickness') <= 3 then
  begin
    SysRouteInsertStepExt('ImmTin_AlkRinse', 'ImmTin_AlkRinse01');

    if Fnc.SysGetAttrValue('CappedVia') = true then
    begin
      if Fnc.SysGetAttrValue('Min_Surf_Plt') >= 18 then
        SysRouteInsertStepExt('MicroEtch_ImmTin', 'MicroEtch_ImmTin01');
    end
    else
      SysRouteInsertStepExt('MicroEtch_ImmTin', 'MicroEtch_ImmTin01');
  end;

  SysRouteInsertStepExt('OffLine_PreTreat', 'OffLine_PreTreat01');
  SysRouteInsertStepExt('PreFinish_Insp', 'PreFinish_Insp01');
  SysRouteInsertStepExt('Immersion_Tin', 'Immersion_Tin01');
  SysRouteInsertStepExt('Immersion_Tin_Bake_Norm', 'Immersion_Tin_Bake_Norm01');

end;

function NeedsFinalBaking : boolean;
begin
  result := false;

  if (
      (
       (MaterialGetsBaking = true) or
       (Fnc.SysGetAttrValue('Lead_Free') = true) or
       (CustCode_Rule('Customer') = 'HJ')
      )
      or
      (                                      
       (Fnc.SysGetAttrValue('Board_Finish') = 'Full Body Gold') or 
       (Fnc.SysGetAttrValue('Board_Finish') = 'Thin/Thick Au Plate') or // 1256 NEED TO CHECK THIS
       //(Fnc.SysGetAttrValue('Sel_Gold_Type') = 'BGA, Encapsulation') or  1256
       // (Fnc.SysGetAttrValue('Sel_Gold_Type') = 'Pattern (Overhang)') or 1256
       (Fnc.SysGetAttrValue('Sel_Gold_Type') = 'Pattern Overhang / PTH') or  // 1256 Need to check this
       //(Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Lead Line Etching B4 Res (Sel)') or 1256
       (Fnc.SysAttrEditGetAttrValue('Gold_Fgr_Type') = 'Lead Line Etching') or
       //(Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Chamfer B4 Res (Sel)') or 911 
       // (Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'After Pattern (Sel)') or   1256
       (Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'NO Lead Line Etching') or
       //(Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Lead Line Etching Af Res (Sel)') or 1256
       (Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Au Finger Plate after SM') or
       //(Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Chamfer Af Res (Sel)') or 1256
       (Fnc.SysGetAttrValue('Board_Finish') = 'ENIG') or
       //(Fnc.SysGetAttrValue('Board_Finish') = 'ENIG + OSP') or // 1173 Additional Surface Finishing. Selective ENIG + OSP 
       ( ENIGplusOSP ) or // 1256
       (Fnc.SysGetAttrValue('Board_Finish') = 'ENEPIG') //or
       // (Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Thin/Thick - Lead Line Etching B4 Res (Sel)') or 1256
       //(Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Thin/Thick - Chamfer B4 Res (Sel)')  911
       // (Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Thin/Thick - After Pattern (Sel)') 1256
      )
      or                                  
      ((Fnc.SysGetAttrValue('Board_Finish') = 'HAL') or  
       (Fnc.SysGetAttrValue('Board_Finish') = 'Lead Free HAL') or 
       (Fnc.SysGetAttrValue('Board_Finish') = 'LB Free HAL + Immersion silver')) // 1322
  and                             
      (HasSolderMaskType('PSR4000-AM10') = false )
     )
  then
    result := true;
end;

function LeadFreeHALWithGoldTaping : boolean;
begin
  result := false;                          
  
  if ( Fnc.SysGetAttrValue('Board_Finish') = 'Lead Free HAL' ) and
     ( SelectiveGoldorFinger ) and
     ( Fnc.SysGetAttrValue('Lead_Free_HAL_Process') = 'Taping' )
  then
    result := true;
end
                                   
procedure RouteFlow04;
//var
  //HoleCheckDone : boolean;                                          
begin                                 
 // HoleCheckDone := false;  ENIG Changes

  if ((Fnc.SysGetAttrValue('Board_Finish') <> 'HAL') and  (Fnc.SysGetAttrValue('Board_Finish') <> 'Lead Free HAL')) then
    SecondaryPlug;

  if Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Tab Plating' then
  begin
    if Fnc.SysGetAttrValue('BackDrill') = true then
    begin
      if (
          ((Fnc.SysGetAttrValue('OSP_Type') = 'LEAD FREE WPF21') or
          (Fnc.SysGetAttrValue('OSP_Type') = 'LEAD FREE WPF21 ORGNC') or
          (Fnc.SysGetAttrValue('OSP_Type') = 'LEAD FREE F2LX'))
         and
          (
           ( Fnc.SysGetAttrValue('Board_Finish') = 'OSP' ) or 
           //( Fnc.SysGetAttrValue('Board_Finish') = 'ENIG + OSP' ) // 1173 Additional Surface Finishing. Selective ENIG + OSP 
           ( ENIGplusOSP ) //1256
          ) 
         )
      then
      begin
        if ((Fnc.SysGetAttrValue('BackDrill') = true) and (BackDrillDone = false)) then
          Back_Drill;
      end;
      else
        ShowMessage('ERROR!!! - UNDEFINED PROCESS FOR TAB PLATING WITH BACK DRILL AND NON-LEAD FREE OSP');
    end;

    if VCUT_TYPE = 'Panel' then
    begin
      if Fnc.SysGetAttrValue('Gold_Tab_Rows') > 2 then
      begin
        if Fnc.HasImpedance = true then
        begin            
          if not HoleCheckDone then
          begin
            SysRouteInsertStepExt('Hole_Check', 'Hole_Check01');
            HoleCheckDone := true;
          end;
          SysRouteInsertStepExt('Impedance', 'Impedance01');
          ImpedanceDone := true;
        end;

        VCut;
                        
        SysRouteInsertStepExt('Gold_Tab_Plating', 'Gold_Tab_Plating01');
      end
      else
        SysRouteInsertStepExt('Gold_Tab_Plating', 'Gold_Tab_Plating01');
    end
    else
      SysRouteInsertStepExt('Gold_Tab_Plating', 'Gold_Tab_Plating01');
                                                        
    {if ( Fnc.SysGetAttrValue('Board_Finish') = 'Immersion Silver' ) and ( Fnc.SysGetAttrValue('Imm_Silver_Process') = 'Taping' )      // 1256
    begin
      SysRouteInsertStepExt('Gold_Finger_Taping', 'Gold_Finger_Taping01'); 
      SysRouteInsertStepExt('Baking', 'BakingGFT');
      SysRouteInsertStepExt('OffLine_PreTreat', 'OffLine_PreTreatGFT');    
    end;}                  
  end;                         

  // 1322 LB Free HAL + Imm Ag
  if Fnc.SysGetAttrValue('Board_Finish') = 'LB Free HAL + Immersion silver' then
    SysRouteInsertStepExt('Immersion_Silver', 'Immersion_Silver01');    

  if (Fnc.SysGetAttrValue('Board_Finish') = 'HAL') or  
     ((Fnc.SysGetAttrValue('Board_Finish') = 'Lead Free HAL') and ( Not SelectiveGoldorFinger )) or
     (Fnc.SysGetAttrValue('Board_Finish') = 'LB Free HAL + Immersion silver') then
  begin 
    SysRouteInsertStepExt('HAL', 'HAL01');

    if HoleCheckDone = false then
      SysRouteInsertStepExt('Hole_Check', 'Hole_Check01');
                         
    SecondaryPlug;        
  end
  else
  begin
    //if ((ENIG_NFluxDone = true) and (Fnc.SysGetAttrValue('BackDrill') = true)) then
    //  SysRouteInsertStepExt('Hole_Check');
    if ( not ENIG_NFluxDone ) and ( not LeadFreeHALWithGoldTaping ) then
    begin
      //if Fnc.SysGetAttrValue('BackDrill') = false then    973 - Hole Check After Backdrill Step
      //begin
        if not HoleCheckDone then
        begin
          SysRouteInsertStepExt('Hole_Check', 'Hole_Check01');
          HoleCheckDone := true;
        end;                 
      //end;
    end;
  end;
  
  //if ( Fnc.SysGetAttrValue('Board_Finish') = 'Immersion Silver' ) and ( Fnc.SysGetAttrValue('Imm_Silver_Process') = 'Taping' ) then      // 1256
  //  SysRouteInsertStepExt('Gold_Finger_Taping', 'Gold_Finger_Taping01'); 
                   
  if Fnc.SysGetAttrValue('Carbon_Ink') = true then
    SysRouteInsertStepExt('Carbon_Ink', 'Carbon_Ink01');

  if (
      ((Fnc.SysGetAttrValue('BackDrill') = true) and (BackDrillDone = false)) and
      (Fnc.SysGetAttrValue('Board_Finish') <> 'Immersion Tin')
     )               
  then //Check reverse condition is true
    Back_Drill;

  if Fnc.SysGetAttrValue('Board_Finish') = 'Immersion Tin' then
    Immersion_TinPt2;
                             
  if ( Fnc.SysGetAttrValue('Board_Finish') = 'Immersion Silver' ) and
     ( SelectiveGoldorFinger ) and
     ( Fnc.SysGetAttrValue('Imm_Silver_Process') = 'Taping' ) then    // 1256 - Comment from Herole
  begin                    
    SysRouteInsertStepExt('Imm_Silv_Taping', 'Imm_Silv_Taping01'); 
    Immersion_Silver_Sel_Gold('Taping');                    
  end;
  
  if LeadFreeHALWithGoldTaping then  // 1355
  begin                      
    SysRouteInsertStepExt('LFH_Taping', 'LFH_Taping01'); 
    Lead_Free_HAL_Sel_Gold('Taping');
    SecondaryPlug; // 1469
  end;
                             
  if (Fnc.SysGetAttrValue('Has_Impedance') = true) and (ImpedanceDone <> true) then
    SysRouteInsertStepExt('Impedance', 'Impedance01');
                               
  if (
      ((Fnc.SysGetAttrValue('Comp_Type') = 'RESISTANCE') and (Fnc.SysGetAttrValue('Line_Width') <> 0.0)) or
      (Fnc.SysGetAttrValue('Comp_Type') = 'CAPACITANCE & RESISTANCE')
     )
  then
     SysRouteInsertStepExt('Resistance_Test', 'Resistance_Test01');

  if ((VCUT_TYPE = 'Panel') and (VCutDone = false))
  then
    VCut;                  

  if Fnc.SysGetAttrValue('NPCountersink') = true then
    SysRouteInsertStepExt('NPCounterSink', 'NPCounterSink01');             

  if Fnc.SysGetAttrValue('Chamfer_Type') = 'Panel Routed' then
  begin
    SysRouteInsertStepExt('PnlRte_Chamfer_PR', 'PnlRte_Chamfer_PR01');
    SysRouteInsertStepExt('PnlRte_Chamfer', 'PnlRte_Chamfer01');
  end;

  if Fnc.SysGetAttrValue('Milling_Type') = 'Edge Milling'  then
  begin
    SysRouteInsertStepExt('Edge_Milling_PR', 'Edge_Milling_PR01');
    SysRouteInsertStepExt('Edge_Milling', 'Edge_Milling01');
  end;

  if Fnc.SysGetAttrValue('Milling_Type') = 'Milling'  then
    SysRouteInsertStepExt('Milling', 'Milling01');

  if Fnc.SysGetAttrValue('Counterbore') = true  then
    SysRouteInsertStepExt('Counterbore', 'Counterbore01');

  if Fnc.SysGetAttrValue('PunchB4EC') = true  then
  begin
    SysRouteInsertStepExt('Punching', 'Punching01');

    if Fnc.SysGetAttrValue('Punch_Type') = '2 Stage Punch' then
      SysRouteInsertStepExt('Punching_X2', 'Punching_X2');
  end;

  if (
      (Fnc.SysGetAttrValue('EC_Type') = 'Panel - Before Primary Routing') or
      (Fnc.SysGetAttrValue('EC_Type') = 'Panel - Before Routing') or
      (Fnc.SysGetAttrValue('EC_Type') = 'Panel - Before Punching')
     )
  then
     SysRouteInsertStepExt('Panel_EC', 'Panel_EC01');

  if (Fnc.SysGetAttrValue('Board_Finish') <> 'HAL') and  
     (Fnc.SysGetAttrValue('Board_Finish') <> 'Lead Free HAL') and
     (Fnc.SysGetAttrValue('Board_Finish') <> 'LB Free HAL + Immersion silver') // 1322
  then
  begin
    if HoleCheckDone = false then
      SysRouteInsertStepExt('Hole_Check', 'Hole_Check01');
  end;           

  SysRouteInsertStepExt('Routing', 'Routing01');

  if ((Fnc.SysGetAttrValue('PunchB4EC') = false) and (Fnc.SysGetAttrValue('Punch_Type') <> 'None'))  then
  begin
    SysRouteInsertStepExt('Punching', 'Punching01');

    if Fnc.SysGetAttrValue('Punch_Type') = '2 Stage Punch' then
      SysRouteInsertStepExt('Punching_X2', 'Punching_X201');
  end;

  if ((Fnc.SysGetAttrValue('VCut_Type') = 'PIECE V-CUT') and (VCutDone = false))  then
    VCut;

  if Fnc.SysGetAttrValue('Chamfer_Type') = 'Piece' then
    SysRouteInsertStepExt('Piece_Chamfer', 'Piece_Chamfer01');

  SysRouteInsertStepExt('Final_Rinse', 'Final_Rinse01');

  SysRouteInsertStepExt('InkJet', 'InkJet01');

  if Fnc.SysGetAttrValue('EC_Type') = 'Part' then
    SysRouteInsertStepExt('Part_EC', 'Part_EC01');

  // 1310 Additiona Backdrill EC
  if ( Fnc.SysGetAttrValue('BackDrill') ) and
     ( Fnc.SysGetAttrValue('BackDrill_EC') ) and 
     ( not GetsHiPot )
  then
    SysRouteInsertStepExt('BackDrill_EC', 'BackDrill_EC01');

  if Fnc.SysGetAttrValue('FourWireMsm') then
    SysRouteInsertStepExt('EC_FlyingProbe', 'EC_FlyingProbe01');
                    
  if GetsHiPot then // 1310 Additiona Backdrill EC
  begin
    SysRouteInsertStepExt('HiPotTest', 'HiPotTest01');
                                   
    if ( Fnc.SysGetAttrValue('BackDrill') ) and ( Fnc.SysGetAttrValue('BackDrill_EC') ) then
      SysRouteInsertStepExt('BackDrill_EC', 'BackDrill_EC01');  
  end;

  if Fnc.SysGetAttrValue('TwoDBarcode') = true then
    SysRouteInsertStepExt('TwoDBarcode', 'TwoDBarcode01');

  if  (( CustCode_Rule('Customer') = 'HQ' ) and ( Fnc.SysGetAttrValue('LayupThickness') >= 2.29 )) or
      ( WarpageLessThanPointFivePct ) or
      (Fnc.SysGetAttrValue('LayupThickness') <= 0.6) or
      (Fnc.SysGetAttrValue('DeWarp') = true)
  then
    SysRouteInsertStepExt('DeWarp', 'DeWarp02');

   //Per Immersion Silver changes from Mani  Jan 2016
   if ( Fnc.SysGetAttrValue('Board_Finish') = 'Immersion Silver' ) and ( not SelectiveGoldorFinger ) then
     Immersion_Silver; 

  if ( Fnc.SysGetAttrValue('Board_Finish') = 'OSP' ) or ( ENIGplusOSP ) then // 1173 Additional Surface Finishing. // 1256
  begin
    if BakingDone = false then          
      Baking;                                                                 

    if (( BakingDone = false ) and ( NeedsFinalBaking )) then
    begin
      SysRouteInsertStepExt('Baking', 'Baking01');
      BakingDone := true;
    end;
                          
    SysRouteInsertStepExt('OSP', 'OSP01');
  end;                        

  {Final Inspection Steps}
  if BakingDone = false then
  begin
    if NeedsFinalBaking then
    begin           
      SysRouteInsertStepExt('Baking', 'Baking01');
      BakingDone := true;

      if  (( Fnc.SysGetAttrValue('Gold_Fgr_Type') <> 'None' ) and ( Fnc.SysGetAttrValue('Gold_Fgr_Type') <> 'Tab Plating' )) or
          ( Fnc.SysGetAttrValue('Sel_Gold_Type') <> 'None' ) or
          (( Fnc.SysGetAttrValue('Board_Finish') = 'Immersion Silver' ) and ( Fnc.SysGetAttrValue('Imm_Silver_Process') = 'Taping' ))  
      then                              
        SysRouteInsertStepExt('OffLine_PreTreat', 'OffLine_PreTreat02');
    end;
  end;

  if (( Fnc.SysGetAttrValue('Board_Finish') = 'Immersion Silver' ) and (Fnc.SysGetAttrValue('CM_Plus') = true )) then
    SysRouteInsertStepExt('PreFinish_Insp', 'PreFinish_Insp01')
  else
    SysRouteInsertStepExt('Final_Insp', 'Final_Insp01');

  if (
      (                                             
       //(Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Lead Line Etching B4 Res (Sel)') or  1256
       (Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Lead Line Etching') or
       (Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'No Horn - Lead Line Etching B4 Res (Sel)') or
       //(Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Chamfer B4 Res (Sel)') or  911
       // (Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'After Pattern (Sel)') or 1256
       (Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'NO Lead Line Etching') or
       //(Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Thin/Thick - Lead Line Etching B4 Res (Sel)') or 1256
       //(Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Thin/Thick - Chamfer B4 Res (Sel)') or 
       //(Fnc.SysGetAttrValue('Gold_Fgr_Type') = 'Thin/Thick - After Pattern (Sel)') or  1256
       (Fnc.SysGetAttrValue('Sel_Gold_Type') <> 'None')
      )                                                                       
      or
      (ProdCodeLXType) or
      (Fnc.SysGetAttrValue('Design_Type') = 'Flip Chip') or
      ((Eng_JobType = 'Production') and (CustCode_Rule('Customer') = 'BC')) or
      (HardGoldB4Resist)                      
     )
  then
    SysRouteInsertStepExt('Final_Scope_Ins', 'Final_Scope_Ins01');

  if (( Fnc.SysGetAttrValue('Board_Finish') = 'Immersion Silver' ) and (Fnc.SysGetAttrValue('CM_Plus') = true )) then
  begin
    SysRouteInsertStepExt('CMPlus_Coating', 'CMPlus_Coating01');
    SysRouteInsertStepExt('Final_Insp', 'Final_Insp01');
  end;

  SysRouteInsertStepExt('Packing', 'Packing01');

end; // HUZZAH!
