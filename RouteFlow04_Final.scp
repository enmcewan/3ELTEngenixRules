begin
  if process = 'Taping'  then
  begin                                                                                                        
    SysRouteInsertStepExt('Immersion_Silver', 'Immersion_Silver01');
                                  
    {if BakingDone = false then  - moved to after electrical test per 1256 reply Word Doc
    begin
      Baking;                   
      if BakingDone then
        SysRouteInsertStepExt('OffLine_PreTreat', 'OffLine_PreTreat01'); // Per Imm Silver Changes from Mani Jan 2016 3EPCB
    end;}  
    //ImmersionSilverDone := true;  1256
  end
  else if process = 'Photo' then 
  begin
    Baking;
    
    if BakingDone then
      SysRouteInsertStepExt('OffLine_PreTreat', 'OffLine_PreTreat01');
        
