    SysRouteInsAddlParameter('Erp','Plating Line', Plating_Line,'','', false, '', '', '');  // Include the Electrolytic Machine Name on the process step.

    if ( MechBlindViaSide <> 'None' ) and ( NCRun = 1 ) then    // MEch Blind Vias 1356 
