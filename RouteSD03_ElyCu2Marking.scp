procedure Epoxy_Plug_Common;
var             
  Onsokki, Noda : boolean;
begin                                 
  Onsokki := true;
  Noda := true;

  StepDeptERP('EPXPL-A');

  if (
      (MfgAttr('Panel_Width') > 509) or
      (MfgAttr('Panel_Width') < 200) or
      (MfgAttr('Panel_Length') > 609) or
      (MfgAttr('Panel_Length') < 250) or
      (MfgAttr('LayupThickness') > 5.00) or  // Epoxy Plugging Max Thickness Change
      (MfgAttr('LayupThickness') < 0.40) or
      (MfgAttr('BlindVias') = true) or
      (MfgAttr('BuriedVias') = true) or
      (MfgAttr('CappedVia') = true)
     )
  then
    Onsokki := false;

  if (
      (MfgAttr('Panel_Width') > 525) or
      (MfgAttr('Panel_Width') < 200) or
      (MfgAttr('Panel_Length') > 620) or
      (MfgAttr('Panel_Length') < 200) or
      (MfgAttr('LayupThickness') > 5.00) or  // Epoxy Plugging Max Thickness Change
      (MfgAttr('LayupThickness') < 0.40)
     )
  then                                        
    Noda := false;

  if ((Onsokki = true) and (Noda = true)) then
    StepWCERP('EPXPL-A');

  if ((Onsokki = true) and (Noda = false)) then
    StepWCERP('EPXPL-A0');

  if ((Onsokki = false) and (Noda = true)) then
    StepWCERP('EPXPL-B0');

  if ((Onsokki = false) and (Noda = false)) then
  begin
    ShowMessage('ERROR!!! - NO EPOXY LINE CAN BE USED!!, PLS CHECK WITH PE.');
    StepDeptERP('zzz=====7o');
    StepWCERP('zzz=====7o');
    StepInstructionERP('MISS STEP'); // Missing Precess Step, Please check with DE
  end;

  StepParamERP('Plug Ink', MfgAttr('EpoxyPlugInk'));
  
  // 1107 Runcard Additional Info
  if MfgAttr('MfgPartType') = 'Top' then
    StepParamERP('Plug to Non-plug', MfgAttr('EpoxyPlug_PlugToNonPlugSpacing'));

  // 2203 Epoxy Plugging additional Info 
  StepParamERP('Thickness', FormatFloat('0.000', MfgAttr('LayupThickness')));
  StepParamERP('Panel Size', MfgAttr('Panel_Size'));
  Insert_MinHole_Aspect('Epoxy');
end;

