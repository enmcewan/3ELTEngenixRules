procedure Resist_Common_SD(Res2nd : boolean);
var
  Smudges : string;
  ExposedCu : string;
begin
  case MfgAttr('SMPlugType') of
    'Primary SM Plug A' ,
    'Primary SM Plug AB',
    'Primary SM Plug BA': StepParamERP('Side A Down', MfgAttr('BotLayer'));
    'Primary SM Plug B' : StepParamERP('Side B Down', MfgAttr('TopLayer'));
  end;

  // 1134 Additional Information
  if Res2nd = true then 
  begin
    StepParamERP('2 Times Resist Ink', MfgAttr('TwoXResist_Ink'));
    Fnc.SysSetAttrValue('Soldermask_Type', MfgAttr('Soldermask_Type') + '/' + MfgAttr('TwoXResist_Ink')); 
  end
  else
  begin         
    StepParamERP('Resist Ink', MaskType);
    Fnc.SysSetAttrValue('Soldermask_Type', MaskType ); 
  end; 

  StepParamERP('Min Resist Clearance', MfgAttr('Min_Res_Cl'));
  StepParamERP('Min Resist Thickness', MfgAttr('Min_Res_Th'));

  if MfgAttr('Max_Res_Th') > 0 then
     StepParamERP('Max Resist Thickness', MfgAttr('Max_Res_Th'));

  StepParamERP('Min Resist Partition', MfgAttr('Min_Res_Pa')); 

  if MfgAttr('Smudges') = true then
    Smudges := 'Allowed'
  else
    Smudges := 'Not Allowed';

  if MfgAttr('Exposed_Cu') = true then
    ExposedCu := 'Allowed'
  else
    ExposedCu := 'Not Allowed';

  StepParamERP('Smudges', Smudges);
  StepParamERP('Exposed Cu', ExposedCu);

  if ((MfgAttr('Design_Type') = 'Flip Chip') or (MfgAttr('Design_Type') = 'Egg Crate')) then
    StepParamERP('Design Type', MfgAttr('Design_Type'));

  if ((MfgAttr('Min_Res_Pa') <= 0.1) and (MfgAttr('Press_Thickness') > 2.0)) then
    StepInstructionERP('RESIS001'); // CC2 DEVELOPING

  if ((MfgAttr('Marking_A') = 'None') and (MfgAttr('Marking_B') = 'None')) then
  StepParamERP('Curing', MfgAttr('Curing'));

  if MfgAttr('Res_Insp_Diag') = true then
    StepInstructionERP('RESIS007'); // REMARK:REFER TO RESIST INSPECTION DIAGRAM.
    
  // 927 - Add Final OL Cu thickness, board thickness and panel size is Resist step
  StepParamERP('Cu Thickness', InProcessCuThk); 
  StepParamERP('Board Thickness', FormatFloat('0.000', MfgAttr('LayupThickness'))); 
  StepParamERP('Panel Size', IntToStr(MfgAttr('Panel_Length')) + ' X ' + IntToStr(MfgAttr('Panel_Width'))); 
  
  AddBoardFinishAsAttribute; // 1223 Additional Surface Finish info under Electrolytic Cu step  // 1257 Add to SM
end;
               
