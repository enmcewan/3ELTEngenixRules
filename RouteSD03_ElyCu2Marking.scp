procedure ENIG_SD;
var
  AuThk : float;
begin
  AuThk := 0.0;

  StepDeptERP('AUFLA-A');

  try
    AuThk := StrToFloat(MfgAttr('Sel_Gold_AuSpec2'));

    if AuThk >= 0.076 then
      StepWCERP('AUFLA-A0');
    else
      StepWCERP('AUFLA-A');

  except                            
    ShowMessage('Warning!!! - Unable to convert Au Thickness Spec to a number, ENIG Plating Resource may be incorrect');
    StepWCERP('AUFLA-A');
  end;

  StepParamERP('Type', MfgAttr('Board_Finish'));

  if MfgAttr('Board_Finish') = 'ENEPIG' then
    StepParamERP('Note', 'Use ENEPIG Bath');

  StepParamERP('Area 2', MfgAttr('Sel_Gold_Area2'));
  StepParamERP('Au Spec 2', MfgAttr('Sel_Gold_AuSpec2'));
  StepParamERP('Ni Spec 2', MfgAttr('Sel_Gold_NiSpec2'));
end;

