procedure SMPlugAttributes;
var
  AttrError : boolean;
begin
  AttrError := false;

  if (
      (MfgAttr('SMPlugDepth') <> 'N/A') and
      (MfgAttr('SMPlugJig') <> 'N/A') and
      (MfgAttr('SMPlugMethod') <> 'N/A')
     )
  then
  begin
    StepParamERP('Depth', MfgAttr('SMPlugDepth'));
    StepParamERP('Jig', MfgAttr('SMPlugJig'));
    StepParamERP('Method', MfgAttr('SMPlugMethod'));
  end
  else
  begin
   if (
       (MfgAttr('SMPlugDepth') = 'N/A') and
       (MfgAttr('SMPlugJig') <> 'N/A') and
       (MfgAttr('SMPlugMethod') <> 'N/A')
      )
   then
     AttrError := true;

   if (
       (MfgAttr('SMPlugDepth') <> 'N/A') and
       (MfgAttr('SMPlugJig') = 'N/A') and
       (MfgAttr('SMPlugMethod') <> 'N/A')
      )
   then
     AttrError := true;

   if (
       (MfgAttr('SMPlugDepth') <> 'N/A') and
       (MfgAttr('SMPlugJig') <> 'N/A') and
       (MfgAttr('SMPlugMethod') = 'N/A')
      )
   then
     AttrError := true;

   if (
       (MfgAttr('SMPlugDepth') = 'N/A') and
       (MfgAttr('SMPlugJig') = 'N/A') and
       (MfgAttr('SMPlugMethod') <> 'N/A')
      )
   then
     AttrError := true;

   if (
       (MfgAttr('SMPlugDepth') <> 'N/A') and
       (MfgAttr('SMPlugJig') = 'N/A') and
       (MfgAttr('SMPlugMethod') = 'N/A')
      )
   then
     AttrError := true;

   if (
       (MfgAttr('SMPlugDepth') = 'N/A') and
       (MfgAttr('SMPlugJig') <> 'N/A') and
       (MfgAttr('SMPlugMethod') = 'N/A')
      )
   then
     AttrError := true;

   if AttrError = true then
     ShowMessage('Error! - SM Plug Depth, Jig or Method not set correctly');
  end;
end;

