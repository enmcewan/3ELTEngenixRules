procedure Insert_MinHole_Aspect(process: string);
var
  thicknessAfterPlating,
  minHoleSize_NP : float;                          
begin
  thicknessAfterPlating := MfgAttr('Press_Thickness') + 
                           ( InProcessCuThk/1000 * 2 ) - 
                           ( StrToFloat(GetBaseCuInMicrons(true, false)) / 1000 );
                           
  if ( not PluggedFilledB4Photo ) or ( process = 'Epoxy' ) then  
  begin
    StepParamERP('Min Hole Size', MfgAttr('Min_Hole_Size'));
    StepParamERP('Aspect Ratio', formatFloat('0.0', thicknessAfterPlating / MfgAttr('Min_Hole_Size')));
  end
  else                              
  begin
    if MfgAttr('Abs_Min_Th') > 0 then
      minHoleSize_NP := MfgAttr('Min_Drill_Size_NP') - (MfgAttr('Abs_Min_Th') / 1000*2 )
    else       
      minHoleSize_NP := MfgAttr('Min_Drill_Size_NP') - (MfgAttr('Ave_Min_Th') / 1000*2 );   
                                                                                                                                          
    if MfgAttr('MfgPartType') = 'Top' then // 961 - removal of NP Holes from subs
    begin
      StepParamERP('Min Hole Size NP', minHoleSize_NP );
      StepParamERP('Aspect Ratio', formatFloat('0.0', thicknessAfterPlating / minHoleSize_NP));
    end                        
    else                                     
      StepParamERP('Aspect Ratio', formatFloat('0.0', thicknessAfterPlating / MfgAttr('Min_Hole_Size')));
  
  end;  
end;

procedure OL_Photo_Common_SD(PhotoProcess : string);
var
  ProcessLabel : string;
  Au_Thk1 : float;
  Au_Thk2 : float;        
  thicknessAfterPlating : float;
  photoAspectRatio : float;             
  minHoleSize_NP : float;

begin                               
  ProcessLabel := '';
  Au_Thk1 := 0.0;                      
  Au_Thk2 := 0.0;
  PhotoLine := '';                
  AltLine := '';
  ProdResource := '';
  DesLine := '';
  thicknessAfterPlating := 0.0;
  photoAspectRatio := 0.0;
  minHoleSize_NP := 0.0;

  if photoProcess <> 'ConformalMask' then
    photoStepList.Add(IntToStr(Fnc.SysRouteInsGetStepNum)); // Photo Sequence 

  StepDeptERP('EXPOL-A');

  if ( PhotoProcess <> 'PForm_SBS' ) and (PhotoProcess <> 'IL_Photo_OL_ThinPanel') then    // 1108 OL Photo Process Update  
  begin
    if MfgAttr('LayupThickness') >= 4.2 then // 793 Update Photo process rule
      StepWCERP('EXPOL-A7');
    else
      begin
      Search_Table_PhotoLine(PhotoProcess, PhotoLine, AltLine, ProdResource, DesLine);
                
      if ProdResource <> '' then
        StepWCERP(ProdResource);
      else
      begin
        ShowMessage('Warning!!! - No Production Resource found for ' + PhotoProcess + '. Department Resource assigned');

        StepWCERP('EXPOL-A');
      end;
    end;

    StepParamERP('Alternative Line', AltLine);
  end;

  case PhotoProcess of
    'PattFormation', 'IL_Photo_OL_ThinPanel', 'PForm_SBS' : ProcessLabel := 'Pattern Formation';  // 1108 OL Photo Process Update  
    'DesPattEtch' : ProcessLabel := 'DES Pattern Etch';
    'GoldPattPlating' : ProcessLabel := 'Gold Pattern Plating';
    'GoldFingerPlating' : ProcessLabel := 'Gold Finger Plating';
    'AlkalineLeadlineEtch' : ProcessLabel := 'Alkaline Lead-line Etch';
    'HoleTenting' : ProcessLabel := 'Hole Tenting';
    'AlkalinePattEtch' : ProcessLabel := 'Alkaline Pattern Etch';
    'SelectiveENIG' : ProcessLabel := 'Selective ENIG';       
    'ConformalMask' : ProcessLabel := 'Conformal Mask';
    'DESPattEtchLGA' : ProcessLabel := 'DES Pattern Etch LGA';
    'HoleTentingLGA' : ProcessLabel := 'Hole Tenting LGA';
    'SelectiveFinishing' : ProcessLabel := 'Selective ' + MfgAttr('Board_Finish'); // 1355
  end;

  StepParamERP('Photo Type', ProcessLabel);

  if PhotoProcess <> 'ConformalMask' then
  begin
    //if NOT ((MfgAttr('BlindVias') = true) and (ProdCodeLBlb = false)) then
    //begin
      if ((MfgAttr('Overall_Blown_Val') = 0.0) and (MfgAttr('Min_PTrace_Gap') = 0.0)) then
      begin
        StepParamERP('Line Width', MfgAttr('Line_Width'));
        StepParamERP('Min Gap', MfgAttr('Min_Gap'));
      end                       
      else
      begin
        StepParamERP('Blown Width', MfgAttr('Line_Width_Blown'));
        StepParamERP('Blown Gap', MfgAttr('Air_Gap_Blown'));
      end;

      StepParamERP('Land Area', MfgAttr('Land_Area'));
      StepParamERP('Annular', MfgAttr('Annular'));
      StepParamERP('Design Thickness', FormatFloat('0.000', MfgAttr('LayupThickness')));

      if MfgAttr('Abs_Min_Th') > 0 then
        Fnc.SysSetAttrValue('Min_Hole_Size', Min_Drill - (MfgAttr('Abs_Min_Th')/1000*2 ));
      else
        Fnc.SysSetAttrValue('Min_Hole_Size', Min_Drill - (MfgAttr('Ave_Min_Th')/1000*2 ));

      Fnc.SysSetAttrValue('Min_Hole_Size', Round100(MfgAttr('Min_Hole_Size')));

      thicknessAfterPlating := MfgAttr('Press_Thickness') + ( InProcessCuThk/1000 * 2 ) - ( StrToFloat(GetBaseCuInMicrons(true, false)) / 1000 );

      // 756 Update Aspect ratio calculation for OL layer photo process
      {if not PluggedFilledB4Photo then
      begin
        StepParamERP('Min Hole Size', MfgAttr('Min_Hole_Size'));
        StepParamERP('Aspect Ratio', formatFloat('0.0', thicknessAfterPlating / MfgAttr('Min_Hole_Size')));
      end
      else
      begin
        if MfgAttr('Abs_Min_Th') > 0 then
          minHoleSize_NP := MfgAttr('Min_Drill_Size_NP') - (MfgAttr('Abs_Min_Th') / 1000*2 )
        else
          minHoleSize_NP := MfgAttr('Min_Drill_Size_NP') - (MfgAttr('Ave_Min_Th') / 1000*2 );   
                                                                                                                                              
        if MfgAttr('MfgPartType') = 'Top' then // 961 - removal of NP Holes from subs
        begin
          StepParamERP('Min Hole Size NP', minHoleSize_NP );
          StepParamERP('Aspect Ratio', formatFloat('0.0', thicknessAfterPlating / minHoleSize_NP));
        end                        
        else                                     
          StepParamERP('Aspect Ratio', formatFloat('0.0', thicknessAfterPlating / MfgAttr('Min_Hole_Size')));
      
      end;}
      
      // 2203 Epoxy Pluggin Additional Information - Common with OL Photo so made the above a sub routine
      Insert_MinHole_Aspect('OLPhoto'); 
                                                         
      //No Computation required - already done
      StepParamERP('Max Hole Size', MfgAttr('Max_Hole_Size'));
      StepParamERP('Max P-Hole Size', MfgAttr('Max_PHole_Size'));
                                                
      if MfgAttr('EdgePlating') = true then // 793 Update Photo Process
        StepParamERP('Edge Plating Max Length', MfgAttr('EdgePlating_Max_Length'));

      // 1107 Runcard Process Additional Inf0 - always show slot length
      //if MfgAttr('Slot_Length_PTH') > 6.0 then // 793 Update Photo Process 
      if MfgPartType = 'Top' then 
        StepParamERP('Max Slot Length', MfgAttr('Slot_Length_PTH'));
                      
      if MfgAttr('Design_Type') = 'Flip Chip' then
        StepParamERP('Design', MfgAttr('Design_Type'));
                                                           
      // 919 Add MLB or HDI remarks under OL Photo Step
      if PhotoBoardType <> '' then
         StepParamERP('Board Type', PhotoBoardType);
         
      // 972 - Material and Panel size on OL Photo step
      StepParamERP('Pre-preg Material', MfgAttr('PPGMat'));
      StepParamERP('Core Material', MfgAttr('CoreMat'));
      StepParamERP('Panel Size', IntToStr(MfgAttr('Panel_Length')) + ' X ' + IntToStr(MfgAttr('Panel_Width')));
        

      if Au_Plt_Count >= 1 then       
      begin
        Fnc.SysSetAttrValue('Sel_Gold_AuSpec1', trim(MfgAttr('Sel_Gold_AuSpec1')));
        Fnc.SysSetAttrValue('Sel_Gold_AuSpec2', trim(MfgAttr('Sel_Gold_AuSpec2')));
                
        try
          Au_Thk1 := StrToFloat('0' + Copy(MfgAttr('Sel_Gold_AuSpec1'),1,4));
          Au_Thk2 := StrToFloat('0' + Copy(MfgAttr('Sel_Gold_AuSpec2'),1,4));

          if ( Au_Plt_Count = 1 ) and ( Au_Thk1 <= 0.1 ) then
            StepInstructionERP('AUGAL001'); // REMARK : OFF CHEMICAL

          if ( Au_Plt_Count = 2 ) and (( Au_Thk1 <= 0.1 ) or ( Au_Thk2 <= 0.1 ))  then
            StepInstructionERP('AUGAL001'); // REMARK : OFF CHEMICAL
        except
          ShowMessage('ERROR! : Cannot convert Au Thickness 1 or 2 to a number');
        end;
      end;

      if ((MfgAttr('Design_Type') = 'Flip Chip') or (ProdCodeLX)) then
        StepInstructionERP('EXPOL001'); // REMARK : OFF BUFFING
    //end;
  end;
end;                     

procedure OL_Photo_FSF; // 1114 Fixed Scale Factor Info
var
  fixedDim: string;
  controlRange: string;
begin                                                                                          
  
  fixedDim := 'X = ' + FormatFloat('0.000', FloatToStr(MfgAttr('OLPhoto_FSF_X'))) + '%; ' +
              'Y = ' + FormatFloat('0.000', FloatToStr(MfgAttr('OLPhoto_FSF_Y'))) + '%';                                 
                     
  controlRange := 'X = ' + FormatFloat('0.000', FloatToStr(MfgAttr('OLPhoto_FSF_X') - MfgAttr('OLPhoto_FSF_X_Tol'))) + '~' +
                           FormatFloat('0.000', FloatToStr(MfgAttr('OLPhoto_FSF_X') + MfgAttr('OLPhoto_FSF_X_Tol'))) + '%; ' +
                  'Y = ' + FormatFloat('0.000', FloatToStr(MfgAttr('OLPhoto_FSF_Y') - MfgAttr('OLPhoto_FSF_Y_Tol'))) + '~' +
                           FormatFloat('0.000', FloatToStr(MfgAttr('OLPhoto_FSF_Y') + MfgAttr('OLPhoto_FSF_Y_Tol'))) + '%';
                           
  if MfgAttr('OLPhoto_FSF_X') > 0 then
  begin
    StepParamERP('Fixed Scale Factor', fixedDim);
    StepParamERP('Control Range', controlRange); 
  end;
end;                                                                                                                   
    
